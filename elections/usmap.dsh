		ctext "TN"   63 ty ls "sans" lc
		ctext "IN"   70 ty ls "sans" lc
		ctext "MO"   77 ty ls "sans" lc
		ctext "MD"   84 ty ls "sans" lc
		ctext "WI"   92 ty ls "sans" lc

		y-=vskip
		ty-=vskip
		ctext COchar 11 y COSize "symbol" demcolor
		ctext MNchar 25 y MNSize "symbol" demcolor
		ctext SCchar 36 y SCSize "symbol" gopcolor
