// use stateface as symbol font
deck
	include "statepop.dsh"
	// vmap population to canvas
	min1=WY
	max1=CA
	min2=2.5
	max2=15
	include "statescale.dsh"
	include "mapchar.dsh"

	gopcolor="red"
	demcolor="blue"
	tossupcolor="gray"

	slide
		y=65
		cy=y+5
		ty=y-2
		ls=1
		vskip=13
		lc="rgb(100,100,100)"
		cop=20

		ctext "2020 US Presidential Election" 50 92 3
		ctext "states ordered by population (US census data, 2019 estimate)"  50 88 2 "sans" "gray"
		legend "Republican" 40 83 1 "sans" gopcolor
		legend "Democratic" 55 83 1 "sans" demcolor

		ctext CAchar 11 y CASize "symbol" demcolor
		ctext TXchar 25 y TXSize "symbol" gopcolor
		ctext FLchar 36 y FLSize "symbol" gopcolor
		ctext NYchar 47 y NYSize "symbol" demcolor
		ctext PAchar 56 y PASize "symbol" demcolor
		ctext ILchar 63 y ILSize "symbol" demcolor
		ctext OHchar 70 y OHSize "symbol" gopcolor
		ctext GAchar 77 y GASize "symbol" demcolor cop
		ctext NCchar 84 y NCSize "symbol" gopcolor cop
		ctext MIchar 92 y MISize "symbol" demcolor

		ctext "CA"   11 ty ls "sans" lc
		ctext "TX"   25 ty ls "sans" lc
		ctext "FL"   36 ty ls "sans" lc
		ctext "NY"   47 ty ls "sans" lc
		ctext "PA"   56 ty ls "sans" lc
		ctext "IL"   63 ty ls "sans" lc
		ctext "OH"   70 ty ls "sans" lc
		ctext "GA"   77 ty ls "sans" lc
		ctext "NC"   84 ty ls "sans" lc
		ctext "MI"   92 ty ls "sans" lc

		circle 11 cy CASize demcolor   cop
		circle 25 cy TXSize gopcolor   cop
		circle 36 cy FLSize gopcolor   cop
		circle 47 cy NYSize demcolor   cop
		circle 56 cy PASize demcolor   cop
		circle 63 cy ILSize demcolor   cop
		circle 70 cy OHSize gopcolor   cop
		circle 77 cy GASize demcolor   cop
		circle 84 cy NCSize gopcolor   cop
		circle 92 cy MISize demcolor   cop

		y-=vskip
		ty-=vskip
		ctext NJchar 11 y NJSize "symbol" demcolor
		ctext VAchar 25 y VASize "symbol" demcolor
		ctext WAchar 36 y WASize "symbol" demcolor
		ctext AZchar 47 y AZSize "symbol" demcolor cop
		ctext MAchar 56 y MASize "symbol" demcolor
		ctext TNchar 63 y TNSize "symbol" gopcolor
		ctext INchar 70 y INSize "symbol" gopcolor
		ctext MOchar 77 y MOSize "symbol" gopcolor
		ctext MDchar 84 y MDSize "symbol" demcolor
		ctext WIchar 92 y WISize "symbol" demcolor

		ctext "NJ"   11 ty ls "sans" lc
		ctext "VA"   25 ty ls "sans" lc
		ctext "WA"   36 ty ls "sans" lc
		ctext "AZ"   47 ty ls "sans" lc
